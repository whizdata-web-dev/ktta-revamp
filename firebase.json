  "hosting": [
    {
      "target": "build_development",
      "public": "build_development",
      "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    },
    {
      "target": "build_production",
      "public": "build_production",
      "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
